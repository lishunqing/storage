<view class="area">
    <view class="text" bindtap="connectPrint">
    {{status}}
    </view>
</view>

<block wx:for="{{list}}" wx:key="{{dispatchlistid}}" wx:if="{{connected}}">
  <view class="area" bindtap="toDetail" id="{{index}}">
      <view class="item">
        <text class="item-title">时间</text>
        <text class="item-value">{{item.createtime}}</text>
      </view>
      <view class="item">
        <text class="item-title">地点</text>
        <text class="item-value">{{item.tostore}}</text>
      </view>
      <view class="item" bindtap="toDetail" id="{{index}}">
        <text class="item-title">备注</text>
        <text class="item-value">{{item.remark}}</text>
      </view>
    </view>
</block>
